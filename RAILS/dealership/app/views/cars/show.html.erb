<article>
    <header>
        <h2><%= @car.year %> <%= @car.make %> <%= @car.model %></h2>
    </header>
    <table>
    <tr>
        <th>Color:</th>
        <td><%=@car.color %></td>
    </tr>
    <tr>
        <th>Price:</th>
        <td><%= number_to_currency(@car.price) %>
        </td>
    </tr>
    </table>
</article>
<nav>
    <%= link_to("All Cars", root_path) %> |
    <%= link_to("Edit", edit_car_path(@car)) %>
</nav>
